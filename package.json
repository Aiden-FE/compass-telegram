{
  "name": "@compass-aiden/telegram",
  "version": "0.0.1",
  "description": "基于 Axios 封装的Http请求模块",
  "main": "dist/main.umd.js",
  "jsdelivr": "dist/main.umd.js",
  "unpkg": "dist/main.umd.js",
  "types": "./types/main.es.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./types/main.es.d.ts",
        "require": "./types/main.cjs.d.ts",
        "default": "./types/main.umd.d.ts"
      },
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js",
      "default": "./dist/main.umd.js"
    },
    "./dist/*": "./dist/*",
    "./types/*": "./types/*"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": "Aiden_FE@outlook.com",
  "repository": "",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
    "build": "npm run clean && rollup -c rollup.config.js",
    "lint": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/",
    "test": "jest --coverage",
    "prepublish": "npm run lint && npm run build",
    "clean": "rimraf dist && rimraf types && rimraf coverage"
  },
  "peerDependencies": {
    "axios": ">=1.4.0"
  },
  "dependencies": {
    "axios-retry": "^3.5.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@compass-aiden/eslint-config": "^1.6.1",
    "@rollup/plugin-commonjs": "~23.0.7",
    "@rollup/plugin-json": "~5.0.2",
    "@rollup/plugin-node-resolve": "~15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "~29.2.6",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "~18.7.23",
    "@typescript-eslint/eslint-plugin": "~5.41.0",
    "@typescript-eslint/parser": "~5.41.0",
    "@zerollup/ts-transform-paths": "~1.7.18",
    "axios": "^1.4.0",
    "eslint": "~8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "~29.2.2",
    "prettier": "^2.8.8",
    "rimraf": "~3.0.2",
    "rollup": "~2.79.1",
    "rollup-plugin-cleanup": "~3.2.1",
    "rollup-plugin-serve": "~2.0.2",
    "rollup-plugin-summary": "~1.4.3",
    "rollup-plugin-ts": "~3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "~29.0.5",
    "typescript": "~4.8.4"
  }
}
