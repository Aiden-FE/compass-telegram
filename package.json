{
  "name": "@compass-aiden/telegram",
  "version": "1.1.0",
  "description": "基于Ky封装的Http请求模块",
  "main": "dist/telegram.es.js",
  "types": "types/telegram.es.d.ts",
  "exports": {
    ".": "./dist/telegram.es.js",
    "./dist/": "./dist/"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": "Aiden_FE@outlook.com",
  "repository": "https://github.com/Aiden-FE/compass-telegram",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "fetch",
    "http",
    "http-client",
    "http-request",
    "ky",
    "request"
  ],
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
    "build": "npm run clean && rollup -c rollup.config.js",
    "lint": "eslint src --ext .ts,.js --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "jest --coverage --passWithNoTests",
    "prepublish": "npm run lint && npm run build",
    "clean": "rimraf dist && rimraf types && rimraf coverage"
  },
  "devDependencies": {
    "@compass-aiden/eslint-config": "^1.2.1",
    "@rollup/plugin-commonjs": "~23.0.7",
    "@rollup/plugin-json": "~5.0.2",
    "@rollup/plugin-node-resolve": "~15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jest": "~29.2.6",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "~18.7.23",
    "@typescript-eslint/eslint-plugin": "~5.41.0",
    "@typescript-eslint/parser": "~5.41.0",
    "@zerollup/ts-transform-paths": "~1.7.18",
    "eslint": "~8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "~29.2.2",
    "prettier": "^2.8.4",
    "rimraf": "~3.0.2",
    "rollup": "~2.79.1",
    "rollup-plugin-cleanup": "~3.2.1",
    "rollup-plugin-serve": "~2.0.2",
    "rollup-plugin-summary": "~1.4.3",
    "rollup-plugin-ts": "~3.0.2",
    "ts-jest": "~29.0.5",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "ky": "^0.33.2",
    "ky-universal": "^0.11.0",
    "lodash-es": "^4.17.21"
  }
}
